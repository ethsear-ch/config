+      <xsl:value-of select="@name"/>
+      <xsl:text>&#x09;</xsl:text>
+      <xsl:value-of select="@url"/>
+    </xsl:for-each>
+    <xsl:text>&#x0a;</xsl:text>
+  </xsl:for-each>
+</xsl:template>
+
+</xsl:stylesheet>
